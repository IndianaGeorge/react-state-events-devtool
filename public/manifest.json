{
  "manifest_version": 2,
  "name": "react-state-event devtool",
  "short_name": "RSE DevTool",
  "version": "0.1.0",
  "devtools_page": "devtools.html",
  "content_security_policy": "script-src 'self' 'sha256-oUd9FmFP9+t0dULXa97X+wUfPDH1UsQUuZAMj25j0cc='; object-src 'self'",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "96": "icon96.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab"
  ]
}