{
  "manifest_version": 2,
  "name": "react-state-event devtool",
  "short_name": "RSE DevTool",
  "version": "0.1.0",
  "devtools_page": "devtools.html",
  "content_security_policy": "script-src 'self' 'sha256-Oq8dW/DkoFtmi8Thwn09vHhPhzrja8w7JpBxqCqwUEM='; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "tempicon.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "MacCtrl+Shift+E"
      },
      "description": "not sure"
    }
  },
  "permissions": [
    "activeTab"
  ]
}